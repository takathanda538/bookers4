<h2>User info</h2>
<%= image_tag @user.get_profile_image(100,100) %>
<table class= "table">
  <tbody>
    <tr>
      <th>name</th>
      <th>
      <%= user.name%>
      </th>
    </tr>
    <tr>
      <th>introduction</th>
      <th>
      <%= user.introduction %>
      </th>
    </tr>
    <tr>
      <th>follows</th>
      <th><%= user.followings.count %></th>
    </tr>
    <tr>
      <th>followers</th>
      <th><%= user.followers.count %></th>
    </tr>
  </tbody>
</table>
